{
  "name": "tadasrimkus/opentelemetry-auto-instrumentation",
  "description": "A package for auto-instrumentation with OpenTelemetry",
  "type": "library",
  "require": {
    "php": "^8.0",
    "ext-opentelemetry": "*",
    "open-telemetry/api": "^1.0",
    "open-telemetry/sem-conv": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Buratinas\\OpenTelemetryAutoInstrumentation\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tadas Rimkus",
      "email": "tadas.rimkus@rtl.nl"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
